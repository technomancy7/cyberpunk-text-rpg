{
    "winsize": [
        640,
        480
    ],
    "autoclose_prompt": false,
    "skip_intro": true,
    "auto": {
        "debug": true
    },
    "keybinds": {
        "autocomplete": "tab",
        "backspace": "backspace",
        "move up": [
            "up",
            "w"
        ],
        "move down": [
            "down",
            "s"
        ],
        "move left": [
            "left",
            "a"
        ],
        "move right": [
            "right",
            "d"
        ],
        "toggle fullscreen terminal": "f1",
        "toggle input focus": "f2",
        "focus input": "t",
        "unfocus input": "escape",
        "send input": "return",
        "quit": "f12"
    }
}